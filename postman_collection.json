							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Response has api_token\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('api_token');",
									"    pm.collectionVariables.set(\"api_token\", jsonData.api_token);",
									"});",
									"",
									"pm.test(\"User data is correct\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.user.name).to.eql(\"Test User\");",
									"    pm.expect(jsonData.user.email).to.eql(\"test@example.com\");",
									"});"
								],
								"type": "text/javascript"
							}
						}
